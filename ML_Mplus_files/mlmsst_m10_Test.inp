TITLE: Your title goes here
DATA: FILE = "ML_Mplus_files/mlmsst_m10_Test.dat";
VARIABLE: 
NAMES = subjn time worry fear sad; 
MISSING=.;
USEVAR = worry fear sad;
CLUSTER = subjn;

ANALYSIS:
TYPE = TWOLEVEL;
ESTIMATOR = ML;
ITERATIONS = 500000;

MODEL:

%WITHIN%

! Latent state variable
eta BY worry@1
fear (las2)
sad (las3) ;

! Latent errors and latent state variances
worry(o_err1) ;
fear(o_err2) ;
sad(o_err3) ;
eta(s_var) ;

%BETWEEN%

! Indicator trait variables
theta1  BY  worry@1 ;
theta2  BY  fear@1 ;
theta3  BY  sad@1 ;

! Latent trait variable
theta BY theta1@1
theta2 (lat2)
theta3 (lat3) ;

! Means and Intercepts
[worry@0] ;
[fear@0] ;
[sad@0] ;
[theta1@0] ;
[theta2*] (int2) ;
[theta3*] (int3) ;
[theta*] ;

! Fixing level-2 residual variances at 0 and latent trait variance
worry@0;
fear@0;
sad@0;
theta1@0;
theta2@0;
theta3@0;
theta(t_var) ;
